@model SatyarthMVCApp.Models.Student

<h2>Student Details</h2>

<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <td>@Model.StudentId</td>
        </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Branch</th>
        <td>@Model.Branch</td>
    </tr>
    <tr>
        <th>Section</th>
        <td>@Model.Section</td>
    </tr>
    <tr>
        <th>Gender</th>
        <td>@Model.Gender</td>
    </tr>

    </table>